{
  "name": "@markpthomas/aisc_360_10",
  "version": "0.0.1",
  "description": "",
  "keywords": [],
  "homepage": "https://github.com/MarkPThomas/mark.se#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarkPThomas/mark.se.git"
  },
  "author": "Mark P Thomas <MarkPorterThomas@gmail.com> (https://github.com/MarkPThomas)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MarkPThomas/mark.se/issues"
  },
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "types": "./dist/index.d.ts",
  "directories": {
    "src": "src",
    "dist": "dist",
    "bin": "bin",
    "doc": "doc",
    "coverage": "coverage",
    "testResults": "testResults"
  },
  "files": [
    "dist/**/**",
    "docs/**/**",
    "!bin/**/**",
    "!**/*.*spec.*",
    "!**/*.json",
    "!**/*.tsbuildinfo",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc --skipLibCheck --allowJs",
    "build:dev": "webpack --config webpack.config.js --node-env development",
    "build:prod": "webpack --config webpack.config.js --node-env production",
    "pretest": "echo pretest script is running... && npm run build",
    "test": "cross-env NODE_ENV=test && jest --coverage --testResultsProcessor=jest-sonar-reporter",
    "test:u": "cross-env NODE_ENV=test node --expose-gc ./node_modules/.bin/jest --config jest.config.ts --runInBand --forceExit",
    "test:i": "node --expose-gc ./node_modules/.bin/jest --config jest.i.config.ts --runInBand --forceExit --testTimeout=7000",
    "test:i:dev": "make run_integration_tests",
    "test:all": "yarn test:u && yarn test:i:dev",
    "test:results": "open-cli ./testResults/coverage/lcov-report/index.html",
    "posttest": "echo posttest script is running... && npm-run-all clear:dev test:results",
    "lint": "eslint ./src",
    "publish": "npm publish --access public",
    "publish:dry": "npm publish --dry-run",
    "clear": "rimraf dist",
    "clear:build": "rimraf ./tsconfig.tsbuildinfo",
    "clear:dev": "npm-run-all clear clear:build",
    "docs": "typedoc --out docs --name aisc_360_10",
    "docs:copy": "./bin/update-docs.sh",
    "docs:update": "npm run docs && npm run docs:copy"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^28.1.4",
    "@types/leaflet": "^1.9.3",
    "@types/node": "^18.0.3",
    "@typescript-eslint/parser": "^5.30.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.19.0",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^29.6.2",
    "jest-sonar-reporter": "^2.0.0",
    "node-polyfill-webpack-plugin": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "open-cli": "^7.0.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.2",
    "ts-node-dev": "^2.0.0",
    "typedoc": "^0.25.7",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-merge": "^5.8.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jestSonar": {
    "sonar56x": true,
    "reportPath": "testResults",
    "reportFile": "sonar-report.xml",
    "indent": 4
  }
}